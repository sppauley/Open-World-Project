[
    {
        "name": "Enemy Stat Reqs",
        "trigger": "level_start",
        "level_nid": null,
        "condition": "any(game.get_enemy_units())",
        "commands": [],
        "only_once": false,
        "priority": 21,
        "_source": [
            "for;ENEMY;game.get_enemy_units(False)",
            "    for;REQS;[[out.value for out in i.components if out.nid == 'stat_req'] for i in u('{ENEMY}').nonaccessories if 'stat_req' in [c.nid for c in i.components]]",
            "            for;REQ;{REQS}[0]",
            "                if;u('{ENEMY}').get_stat({REQ}[0]) < {REQ}[1]",
            "                    set_stats;{ENEMY};{eval:{REQ}[0]},{eval:{REQ}[1]};immediate",
            "                end",
            "            endf",
            "    endf",
            "endf"
        ]
    }
]